{
  "name": "jobs-startup",
  "version": "1.0.0",
  "description": "Fullstack приложение для поиска вакансий",
  "private": true,
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:build": "docker-compose -f docker-compose.dev.yml build",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev:clean": "docker-compose -f docker-compose.dev.yml down -v --rmi all",

    "build": "docker-compose build",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "clean": "docker-compose down -v --rmi all"
  }
}
