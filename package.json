{
  "name": "jobs-startup",
  "version": "1.0.0",
  "description": "Fullstack приложение для поиска вакансий",
  "private": true,
  "scripts": {
    "start:dev": "powershell -ExecutionPolicy Bypass -File ./scripts/dev/start.ps1",
    "stop:dev": "powershell -ExecutionPolicy Bypass -File ./scripts/dev/stop.ps1",

    "build": "powershell -ExecutionPolicy Bypass -File ./scripts/prod/build.ps1",
    "start": "powershell -ExecutionPolicy Bypass -File ./scripts/prod/start.ps1",
    "stop": "powershell -ExecutionPolicy Bypass -File ./scripts/prod/stop.ps1",
    "restart": "powershell -ExecutionPolicy Bypass -File ./scripts/prod/restart.ps1",
    "clean": "powershell -ExecutionPolicy Bypass -File ./scripts/prod/clean.ps1"
  }
}
